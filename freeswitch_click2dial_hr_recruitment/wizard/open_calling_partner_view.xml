<openerp>
<data>

<!-- Add button on the "Open Calling partner" wizard -->
<record id="add_open_hr_button" model="ir.ui.view">
    <field name="name">freeswitch_click2dial_hr.add_open_registrations_button</field>
    <field name="model">wizard.open.calling.partner</field>
    <field name="inherit_id" ref="freeswitch_click2dial.view_open_calling_partner"/>
    <field name="arch" type="xml">
        <group name="partner_found" position="inside">
            <button name="open_registrations" string="Open Related Registrations" type="object" icon="gtk-go-forward" />
        </group>
    </field>
</record>

</data>
</openerp>
